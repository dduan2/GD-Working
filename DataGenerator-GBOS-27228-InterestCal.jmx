          <stringProp name="query">Declare @pAccountKey bigint;
Declare @pAccountBalKey bigint;
Declare @pDate datetime;
Declare @pIndx int;
select @pAccountKey=AccountKey from account where accountidentifier=&apos;${AccountIdentifier}&apos;--&apos;47377c34-147a-4a1b-ab5f-70ee9bf04a49&apos;
 
--Get accountbalancekey using accountkey
select @pAccountBalKey=AccountBalanceKey from AccountBalance where accountkey=@pAccountKey and accountbalancetypekey=1
 
--Insert the necessary test data into AccountBalanceInterest and ProcessorAccountBalanceSnapshot tables
IF NOT EXISTS(SELECT * FROM AccountBalanceInterest where AccountBalanceKey=@pAccountBalKey and InterestYieldStartDate &lt; GETDATE())
BEGIN
	INSERT INTO AccountBalanceInterest
	(
		OverrideAPY,
		OverrideReason,
		InterestYieldStartDate,
		InterestYieldEndDate,
		AccountBalanceKey,
		ProductInterestTierKey
	)
	VALUES
	(1.1, &apos;TestingReg1&apos;, GETDATE()-1, NULL, @pAccountBalKey, 1),
	(1.2, &apos;TestingReg2&apos;, GETDATE()-2, NULL, @pAccountBalKey, 2);
END

SET @pIndx=0;
WHILE(@pIndx &lt; 5)
BEGIN
 set @pDate = GETDATE()-(ABS(CHECKSUM(NEWID()) % 5) + 1);
    INSERT INTO dbo.ProcessorAccountBalanceSnapshot
(
    CreateDate,
    ChangeDate,
    ChangeBy,
    AccountKey,
    AccountBalanceKey,
    ProcessorBusinessDate,
    LedgerBalance,
    LedgerBalanceAsOfDate,
    AvailableBalance,
    AvailableBalanceAsOfDate
) VALUES
	(   @pDate,     -- CreateDate - datetime
    @pDate,     -- ChangeDate - datetime
    &apos;varsha&apos;,        -- ChangeBy - nvarchar(100)
    @pAccountKey,         -- AccountKey - bigint
    @pAccountBalKey,         -- AccountBalanceKey - bigint
    @pDate,     -- ProcessorBusinessDate - date
    CONVERT(MONEY,RAND()*1000),       -- LedgerBalance - money
    @pDate, -- LedgerBalanceAsOfDate - datetime2(7)
    CONVERT(MONEY,RAND()*500),          -- AvailableBalance - money
    @pDate  -- AvailableBalanceAsOfDate - datetime2(7)
    )
    SET @pIndx += 1
END

update accountbillcycle
