          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.commons.lang.RandomStringUtils;

try{
	def nickName = RandomStringUtils.randomAlphabetic(8)
	def MerchantName= RandomStringUtils.randomAlphabetic(8)
	def name = RandomStringUtils.randomAlphabetic(8)
	def accountNumber = RandomStringUtils.randomNumeric(5)
	
	def address1 = RandomStringUtils.randomNumeric(5) + &apos; &apos; + &apos; Foothill Blvd&apos;
	def address2 = &apos;SUITE &apos; + RandomStringUtils.randomNumeric(3)
	def zipCode = &apos;91&apos; + RandomStringUtils.randomNumeric(3)
	
	def email = RandomStringUtils.randomAlphabetic(15)+&apos;@greendotcorp.com&apos;
	def phoneNumber = &apos;6505&apos; + RandomStringUtils.randomNumeric(6)

	// Personal Data
	vars.put(&quot;payeeType&quot;, &quot;Person&quot;) // &apos;Merchant&apos;
	vars.put(&quot;name&quot;, name)
	vars.put(&quot;nickName&quot;, nickName)
	vars.put(&quot;address1&quot;, address1)
	vars.put(&quot;address2&quot;, address2)
	vars.put(&quot;city&quot;, &quot;Pasadena&quot;)
	vars.put(&quot;state&quot;, &quot;CA&quot;)
	vars.put(&quot;country&quot;,&quot;USA&quot;)
	vars.put(&quot;zip&quot;, zipCode)
	vars.put(&quot;accountNumber&quot;, accountNumber)
	vars.put(&quot;phoneNumber&quot;, phoneNumber)
	vars.put(&quot;email&quot;, email)
	vars.put(&quot;merchantId&quot;,MerchantName)



}catch (Throwable ex)
{
	log.error(&quot;Error in Generate enrollment data&quot;, ex);
	throw ex;
}
</stringProp>
