        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get BillpayAccountData from db" enabled="true">
          <stringProp name="dataSource">BillPay</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">DECLARE @startDate varchar(10)
DECLARE @endDate varchar(10)
set @startDate=CONVERT(VARCHAR(10),GETDATE(),120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE()+10,120)

select top ${NUMBER_OF_ACCOUNTS} BPA.ExternalAccountReferenceID, BPS.BillPayScheduleIdentifier
FROM BillPay.dbo.BillPayPayee BPP (nolock)
INNER JOIN BillPay.dbo.BillPayAccount BPA (nolock) ON BPA.BillPayAccountKey = BPP.BillPayAccountKey
INNER JOIN BillPay.dbo.BillPayPayeeStatus BPPS (nolock) ON BPPS.BillPayPayeeStatusKey = BPP.BillPayPayeeStatusKey
INNER JOIN BillPay.dbo.PayeeType PT (nolock)  ON PT.PayeeTypeKey = BPP.PayeeTypeKey 
INNER JOIN BillPay.dbo.BillPaySchedule BPS (nolock) ON BPS.BillPayPayeeKey = BPP.BillPayPayeeKey
WHERE ScheduleDate between @startDate and @endDate
and ProgramCode= &apos;${ProgramCode}&apos;
order by BPA.ExternalAccountReferenceID desc</stringProp>
