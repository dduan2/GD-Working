        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate enrollment data" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters">${ProductCode},${ProgramCode}</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.commons.lang.RandomStringUtils;

try{

	def accountType = &apos;business&apos;
	def BusinessName =  RandomStringUtils.randomAlphabetic(8)+&quot;Corp&quot;
	def BusinessLegalName =  RandomStringUtils.randomAlphabetic(8)+&quot;LegalNameCorp&quot;
	def BusinessAddressLine1 = RandomStringUtils.randomNumeric(5) + &apos; &apos; + &apos; Foothill Blvd&apos;
	def BusinessAddressLine2 = &apos;SUITE &apos; + RandomStringUtils.randomNumeric(3)
	def BusinessCity = &quot;Pasadena&quot;
	def BusinessState = &quot;CA&quot;
	def BusinessZipCode=RandomStringUtils.randomNumeric(5)
	def BusinessCountryCode =&quot;USA&quot;
	def BusinessType = &quot;soleProp&quot;
	def BusinessTaxID = RandomStringUtils.randomNumeric(9)
	def BusinessIndustry = RandomStringUtils.randomNumeric(5)
	def AvgMonthlyProcessingVolume = RandomStringUtils.randomNumeric(6)
	def BusinessEmailAddress = RandomStringUtils.randomAlphabetic(15)+&apos;@Corp.com&apos;
	def URL = &quot;http://www.acmecorpA7E.com&quot;


	def firstName = RandomStringUtils.randomAlphabetic(8)
	def lastName = RandomStringUtils.randomAlphabetic(8)
	def sSNSuffix = &quot;&quot;
	def ssn = &quot;10&quot; + RandomStringUtils.randomNumeric(7)
//	Kyc cure ssn
//	def ssn = &quot;991&quot; + RandomStringUtils.randomNumeric(6)
	def address1 = RandomStringUtils.randomNumeric(5) + &apos; &apos; + &apos; Foothill Blvd&apos;
	def address2 = &apos;SUITE &apos; + RandomStringUtils.randomNumeric(3)
	def zipCode = &apos;9&apos; + RandomStringUtils.randomNumeric(4)
//	def address1 = &apos;3465 EAST FOOTHILL BLVD&apos; //&apos;3465 E FOOTHILL BLVD&apos;
//	def address2 = &apos;SUITE &apos; + RandomStringUtils.randomNumeric(3)
//	def zipCode = &apos;91107&apos;
	
	def email = RandomStringUtils.randomAlphabetic(10)+&apos;virtual@greendotcorp.com&apos;
	TimeZone.setDefault(TimeZone.getTimeZone(&apos;UTC&apos;))
	def now = new Date()
	now = now.format(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSS&apos;Z&apos;&quot;)

	def RequestId = java.util.UUID.randomUUID()

	vars.put(&quot;TermsAcceptanceDateTime&quot;, now)
	vars.put(&quot;RequestID&quot;, RequestId.toString())


	//Business Data
	vars.put(&quot;businessAccountType&quot;, accountType)
	vars.put(&quot;businessName&quot;, BusinessName)
	vars.put(&quot;businessLegalName&quot;, BusinessLegalName)
	vars.put(&quot;businessAddressLine1&quot;, BusinessAddressLine1)
	vars.put(&quot;businessAddressLine2&quot;, BusinessAddressLine2)
	vars.put(&quot;businessCity&quot;, BusinessCity)
	vars.put(&quot;businessState&quot;, BusinessState)
	vars.put(&quot;businessZipCode&quot;,BusinessZipCode)
	vars.put(&quot;businessCountryCode&quot;, BusinessCountryCode)
	vars.put(&quot;businessType&quot;, BusinessType)
	vars.put(&quot;businessTaxID&quot;, BusinessTaxID)
	vars.put(&quot;businessIndustry&quot;, BusinessIndustry)
	vars.put(&quot;avgMonthlyProcessingVolume&quot;, AvgMonthlyProcessingVolume)
	vars.put(&quot;businessEmailAddress&quot;, BusinessEmailAddress)
	vars.put(&quot;URL&quot;, URL)


	// Personal Data
	vars.put(&quot;Title&quot;, &quot;Mr&quot;)
	vars.put(&quot;FirstName&quot;, firstName)
	vars.put(&quot;MiddleInitials&quot;, &quot;K&quot;)
	vars.put(&quot;LastName&quot;, lastName)
	vars.put(&quot;AddressLine1&quot;, address1)
	vars.put(&quot;AddressLine2&quot;, address2)
	vars.put(&quot;City&quot;, &quot;Pasadena&quot;)
	vars.put(&quot;State&quot;, &quot;CA&quot;)
	vars.put(&quot;ZipCode&quot;, zipCode)
	vars.put(&quot;Type&quot;, &quot;home&quot;)

	//end of Personal Data
	//Personal Data-Part2

	vars.put(&quot;SSN&quot;, ssn)
	vars.put(&quot;SSNSuffix&quot;, sSNSuffix)
	vars.put(&quot;DateOfBirth&quot;, &quot;1987-02-02&quot;)
	vars.put(&quot;Email&quot;, email)
	vars.put(&quot;PhoneNumber&quot;, &quot;6505268188&quot;)
	vars.put(&quot;PhoneType&quot;, &quot;Mobile&quot;)

	//End of Personal Data 2
	//Encrypted Data part
	vars.put(&quot;encryptedUserDataVersion&quot;, RequestId.toString())
	vars.put(&quot;EphemeralPublicKey&quot;, RequestId.toString())
	vars.put(&quot;PublicKeyHash&quot;, RequestId.toString())
	vars.put(&quot;salt&quot;, RequestId.toString())
	vars.put(&quot;Version&quot;, &quot;EC_v1&quot;)
	//vars.put(&quot;ProgramName&quot;, vars.get(&apos;ProgramCode&apos;))


	// Terms acceptance
	vars.put(&quot;TermsIdentifier&quot;, &quot;${__chooseRandom(dda,privPlcy,termId)}&quot;)
	vars.put(&quot;TermsAcceptanceDateTime&quot;, now)
	vars.put(&quot;termsAcceptanceFlag&quot;,&quot;true&quot;)


	// Account Data
	vars.put(&quot;ProductCode&quot;, vars.get(&apos;ProductCode&apos;))
	vars.put(&quot;AccountType&quot;, &quot;Savings&quot;)
	vars.put(&quot;Currency&quot;, &quot;USD&quot;)
	vars.put(&quot;PurseIdentifier&quot;, RequestId.toString())
	vars.put(&quot;PurseType&quot;, &quot;Primary&quot;)
	vars.put(&quot;UserIdentifier&quot;, RequestId.toString())


}catch (Throwable ex)
{
	log.error(&quot;Error in Generate enrollment data&quot;, ex);
	throw ex;
