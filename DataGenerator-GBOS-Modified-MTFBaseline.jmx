        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get AccountData from db" enabled="true">
          <stringProp name="dataSource">GBOS</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">
DECLARE @startDate varchar(10)
DECLARE @endDate varchar(10)
set @startDate=CONVERT(VARCHAR(10),GETDATE()-30,120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE(),120)

SELECT top ${NUMBER_OF_ACCOUNTS} ac.accountIdentifier
, @startDate as startDate
, @endDate as endDate
FROM Account ac
inner join Product p
on ac.ProductKey=p.ProductKey
where ac. AccountIdentifier in 
	(select AccountIdentifier from AuthorizeTransaction at1 WHERE at1.AuthResponseCodeKey =1 
	--and at1.AccountIdentifier = ac.AccountIdentifier 
	and at1.isMatched=0
	and at1.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union
	select AccountIdentifier from AuthorizeTransaction at2 WHERE at2.AuthResponseCodeKey &gt;2 
	--and at2.AccountIdentifier = ac.AccountIdentifier
	and at2.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union
	select AccountIdentifier from PostTransaction at3 
	WHERE at3.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union
	select AccountIdentifier from PostInternalTransaction at4 
	WHERE at4.ProcessorTransactionDate BETWEEN @startDate and @endDate)
and p.ProductCode=&apos;${ProductCode}&apos;
</stringProp>
