          <stringProp name="query">	SELECT  RPR.RetailPartnerIdentifier, FSP.RetailerFee,RPT.RetailPartner
	FROM dbo.RetailPartnerRole RPR (nolock)
	INNER JOIN dbo.RetailPackage RPK (nolock) ON RPK.RetailPartnerRoleKey = RPR.RetailPartnerRoleKey
	INNER JOIN dbo.RetailPartner RPT (nolock) ON RPT.RetailPartnerKey = RPR.RetailPartnerKey
	INNER JOIN dbo.RetailPackageType RPY (nolock) ON RPY.RetailPackageTypeKey = RPK.RetailPackageTypeKey
	INNER JOIN dbo.FeeSplit FSP (nolock) ON FSP.RetailPartnerRoleKey = RPR.RetailPartnerRoleKey
	WHERE GETDATE() &gt;= RPK.StartDate
	AND GETDATE() &lt; RPK.EndDate</stringProp>
