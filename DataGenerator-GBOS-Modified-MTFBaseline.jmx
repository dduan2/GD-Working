          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;

FileWriter fstream = new FileWriter(vars.get (&quot;WORKSPACE_PATH&quot;) + File.separator  + &quot;${RESULT_FILE_NAME}&quot;,true);

String SourceAccountIdentifier = vars.getObject(&quot;SourceAccountIdentifierList&quot;).get(0).get(&quot;accountIdentifier&quot;);
int sourcecount = vars.getObject(&quot;SourceAccountIdentifierList&quot;).size();
int index = 0;
for (var row : vars.getObject(&quot;TargetAccountIdentifierList&quot;))
{
	String SourceAccountIdentifier = vars.getObject(&quot;SourceAccountIdentifierList&quot;).get(index%sourcecount).get(&quot;accountIdentifier&quot;);
	String SourceEmail = vars.getObject(&quot;SourceAccountIdentifierList&quot;).get(index%sourcecount).get(&quot;Email&quot;);
	String SourceFirstName = vars.getObject(&quot;SourceAccountIdentifierList&quot;).get(index%sourcecount).get(&quot;FirstName&quot;);
	String SourceLastName = vars.getObject(&quot;SourceAccountIdentifierList&quot;).get(index%sourcecount).get(&quot;LastName&quot;);
	String SourceUserName = SourceFirstName + &quot; &quot; + SourceLastName;


	String TargetUserName = row.get(&quot;FirstName&quot;) + &quot; &quot; + row.get(&quot;LastName&quot;);
	
	fstream.write(SourceAccountIdentifier + &quot;,&quot; + SourceEmail + &quot;,&quot; + SourceFirstName + &quot;,&quot; + SourceLastName + &quot;,&quot; + SourceUserName + &quot;,&quot; + row.get(&quot;accountIdentifier&quot;) + &quot;,&quot; + row.get(&quot;Email&quot;) + &quot;,&quot; + row.get(&quot;FirstName&quot;) + &quot;,&quot; + row.get(&quot;LastName&quot;) + &quot;,&quot; + TargetUserName + System.getProperty(&quot;line.separator&quot;));
	index++;
}
fstream.close();
</stringProp>
