        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get AccountData from db" enabled="true">
          <stringProp name="dataSource">GBOS</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">select top ${NUMBER_OF_ACCOUNTS} 
a.AccountIdentifier as accountIdentifier
, cp.ConsumerProfileIdentifier as userIdentifier
from Account a
inner join Product p on a.ProductKey=p.ProductKey
join AccountHolder h on (h.AccountKey = a.AccountKey)
JOIN ConsumerProfile cp on cp.ConsumerProfileKey=h.ConsumerProfileKey
JOIN AccountHolderCure hc on h.AccountHolderCureKey=hc.AccountHolderCureKey
join VerificationRequest vr on vr.AccountHolderKey = h.AccountHolderKey
JOIN VerificationActivity va on va.VerificationRequestKey=vr.VerificationRequestKey
join VerificationStatus vs on vs.VerificationStatusKey= vr.VerificationStatusKey
WHERE
	va.VerificationActivityTypeKey=5
and
	vr.VerificationStatusKey=3
and
	a.AccountStatusKey=1
and 
	p.ProductCode = &apos;${ProductCode}&apos;
and
	NOT EXISTS(select * from AccountHolder_PaymentIdentifier ap where ap.AccountHolderKey = h.AccountHolderKey)
	

</stringProp>
