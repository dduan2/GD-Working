        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Append results to output file" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;

FileWriter fstream = new FileWriter(vars.get (&quot;WORKSPACE_PATH&quot;) + File.separator  + &quot;${RESULT_FILE_NAME_P}&quot;,true);



for (var row : vars.getObject(&quot;PaymentInstrumentIdentifierList&quot;))
{
	 String RequestId = java.util.UUID.randomUUID().toString();
	String last4Pan = row.get(&quot;Last4PAN&quot;);
	int lastdig = (Integer.parseInt(last4Pan.substring(3)) +1)%10;
	String pin = last4Pan.substring(0,3)+Integer.toString(lastdig);
	log.info(last4Pan + &quot;---------------&quot; + pin);
	fstream.write(row.get(&quot;AccountIdentifier&quot;) + &quot;,&quot; + row.get(&quot;PaymentInstrumentIdentifier&quot;) + &quot;,&quot; + pin + &quot;,&quot; + RequestId + System.getProperty(&quot;line.separator&quot;));
}
fstream.close();
</stringProp>
