        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get PaymentIddentifierData from db" enabled="true">
          <stringProp name="dataSource">GBOS</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">select top ${NUMBER_OF_ACCOUNTS} b.Bin, pid.TokenizedPan, pid.Last4PAN, a.AccountIdentifier, pin.PaymentInstrumentIdentifier, Pid.PaymentIdentifierProxy,cp.FirstName + &apos; &apos; + cp.MiddleName + &apos; &apos; + cp.LastName as CardHolderName 
from Account a (nolock)  inner join Product p (nolock) on (p.ProductKey = a.ProductKey)  
inner join Program pg (nolock) on (pg.ProgramKey = p.ProgramKey)  
inner join BinProduct bp (nolock) on (bp.productkey = p.productkey)  
inner join Bin b (nolock) on (b.binkey = bp.binkey)  
inner join AccountHolder h (nolock) on (h.AccountKey = a.AccountKey)  
inner join ConsumerProfile cp (nolock) on (cp.ConsumerProfileKey = h.ConsumerProfileKey)  
inner join AccountHolder_PaymentIdentifier ap (nolock) on (ap.AccountHolderKey = h.AccountHolderKey)  
inner join PaymentIdentifier pid (nolock) on (pid.PaymentIdentifierKey = ap.PaymentIdentifierKey)  
inner join PaymentInstrument pin (nolock) on (pin.PaymentIdentifierKey = pid.PaymentIdentifierKey)  
where a.accountstatuskey=2 and pin.PaymentInstrumentTypeKey in (2,3) and h.IsPrimaryAccountHolder = 1  and ap.IsActive = 1 
and pg.programCode=&apos;${ProgramCode}&apos;
and p.productcode= &apos;${ProductCode}&apos;
and a.CreateDate between &apos;${StartDate}&apos; and &apos;${EndDate}&apos;
and pin.PaymentInstrumentIdentifier is not null
order by a.AccountKey desc</stringProp>
