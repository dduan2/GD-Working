        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get AccountData from db" enabled="true">
          <stringProp name="dataSource">DirectDeposit</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">
DECLARE @startDate varchar(10)
DECLARE @endDate varchar(10)
set @startDate=CONVERT(VARCHAR(10),GETDATE()-30,120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE(),120)

SELECT top ${NUMBER_OF_ACCOUNTS} 
		 achT.AccountIdentifier,
		 @startDate as startDate, @endDate as endDate
    FROM ACHTransaction achT (nolock)
        INNER JOIN dbo.AchProcessingBatch apb (nolock) 
            ON apb.AchProcessingBatchKey = achT.AchProcessingBatchKey  
        INNER JOIN dbo.ACHTranCode atc  (nolock)
            ON atc.ACHTranCodeKey = achT.ACHTranCodeKey  
        INNER JOIN dbo.ACHTranCodeType atct  (nolock)
            ON atct.ACHTranCodeTypeKey = atc.ACHTranCodeTypeKey  
               AND atct.ACHTranCodeTypeKey &lt;&gt; 3  
        INNER JOIN dbo.ACHBatch achB  (nolock)
            ON achB.ACHBatchKey = achT.ACHBatchKey  
        INNER JOIN dbo.ACHFileHeader achF  (nolock)
            ON achF.ACHFileHeaderKey = achB.ACHFileHeaderKey  
               AND CAST(achF.ACHFileCreationDate AS DATE) &gt;= @startDate  
               AND CAST(achF.ACHFileCreationDate AS DATE) &lt;= @endDate               
