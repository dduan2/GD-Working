DECLARE @startDate varchar(10)
DECLARE @endDate varchar(10)
--set @startDate=CONVERT(VARCHAR(10),GETDATE()-30,120)
set @startDate=CONVERT(VARCHAR(10),GETDATE()-30,120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE()+1,120)

SELECT top ${NUMBER_OF_ACCOUNTS} ac.accountIdentifier,
ab.AccountBalanceIdentifier
,count(ab.AccountBalanceIdentifier)
, @startDate as startDate
, @endDate as endDate
FROM Account ac with(nolock)
inner join Product p with(nolock)
on ac.ProductKey=p.ProductKey
inner Join AccountHolder As ah with(nolock)
On ah.AccountKey = ac.AccountKey
inner Join AccountHolder_PaymentIdentifier As ahpi with(nolock)
On ahpi.AccountHolderKey = ah.AccountHolderKey
inner Join PaymentIdentifier As pid with(nolock)
On pid.PaymentIdentifierKey = ahpi.PaymentIdentifierKey
inner join AccountBalance ab with(nolock)
on ac.accountkey=ab.accountkey
inner join EventFeedMessage ef with(nolock)
On ef.PaymentIdentifierProxy = pid.PaymentIdentifierProxy
Where  p.ProductCode=&apos;${ProductCode}&apos;
and ac.CreateDate &gt;&apos;${START_DATE}&apos;
and ab.AccountBalanceTypeKey in (1,2)
group by ac.accountIdentifier, ab.AccountBalanceIdentifier
having count(ab.AccountBalanceIdentifier)&gt;=${StartCount} and count(ab.AccountBalanceIdentifier)&lt;=${EndCount}
--Order by ac.CreateDate desc
