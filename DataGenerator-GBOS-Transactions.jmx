set @startDate=CONVERT(VARCHAR(10),GETDATE()-30,120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE()+1,120)

SELECT top ${NUMBER_OF_ACCOUNTS} temp.accountIdentifier
, @startDate as startDate
, @endDate as endDate
FROM Account ac with(nolock)
inner join Product p with(nolock)
on ac.ProductKey=p.ProductKey
inner join 
	(select AccountIdentifier from AuthorizeTransaction at1 with(nolock) WHERE at1.AuthResponseCodeKey =1 
	and at1.isMatched=0
	and at1.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union all
	select AccountIdentifier from AuthorizeTransaction at2 with(nolock) WHERE at2.AuthResponseCodeKey &gt;2 
	and at2.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union all
	select Accountidentifier from AuthorizeTransaction at3
	inner join AuthorizeSubTransaction ast
	on at3.AuthorizeTransactionKey = ast.AuthorizeTransactionKey
	where at3.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union all
	select AccountIdentifier from PostTransaction pt with(nolock)
	WHERE pt.ProcessorTransactionDate BETWEEN @startDate and @endDate
	union all
	select AccountIdentifier from PostInternalTransaction pit with(nolock)
	WHERE pit.ProcessorTransactionDate BETWEEN @startDate and @endDate) Temp
On ac.AccountIdentifier = Temp.Accountidentifier
Where  p.ProductCode=&apos;${ProductCode}&apos;
and ac.CreateDate &gt;&apos;${START_DATE}&apos;
group by temp.AccountIdentifier
having count(temp.AccountIdentifier)&gt;=${StartCount} and count(temp.AccountIdentifier)&lt;=${EndCount}
--Order by ac.CreateDate desc
