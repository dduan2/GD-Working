            <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="GenerateTransferToken" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">false</stringProp>
              <stringProp name="script">import java.util.UUID;
import java.nio.ByteBuffer;
import java.math.*;
import java.nio.charset.Charset;

public static String encryptedString(String guidStr) {
       UUID id = UUID.fromString(guidStr);

       ByteBuffer bb = ByteBuffer.wrap(new byte[16]);
       bb.putLong(id.getMostSignificantBits());
       bb.putLong(id.getLeastSignificantBits());

       byte[] fixedBuffer = new byte[17];
       for (int i = 0; i &lt; 4; i++)
           fixedBuffer[i + 1] = bb.array()[3 - i];
       for (int i = 4; i &lt; 6; i++)
           fixedBuffer[i + 1] = bb.array()[5 - (i - 4)];
       for (int i = 6; i &lt; 8; i++)
           fixedBuffer[i + 1] = bb.array()[7 - (i - 6)];
       for (int i = 8; i &lt; 16; i++)
           fixedBuffer[i + 1] = bb.array()[i];

       
       for (int i = 1; i &lt; 9; i++) {
           byte temp = fixedBuffer[i];
           fixedBuffer[i] = fixedBuffer[fixedBuffer.length - i];
           fixedBuffer[fixedBuffer.length - i] = temp;
       }

       fixedBuffer[0] = 0;

       byte[] newBytes = new byte[fixedBuffer.length + 1];
       //Array.Copy(input, newBytes, input.Length);
       newBytes = fixedBuffer.clone();
       BigInteger bigInt = new BigInteger(newBytes);
       StringBuilder sb = new StringBuilder();

       while (!bigInt.equals(BigInteger.valueOf(0))) {
           BigInteger rem;
           rem = bigInt.remainder(BigInteger.valueOf(36));
           bigInt = bigInt.divide(BigInteger.valueOf(36));
           rem = rem.add(BigInteger.valueOf(48));
           if (rem.compareTo(BigInteger.valueOf(57)) == 1)
               rem = rem.add(BigInteger.valueOf(7));
           int b = rem.intValue();
           sb.insert(0, (char) b);
       }
	return sb.toString();       
   }

String token = encryptedString(&quot;${FundTransferIdentifier}&quot;);
vars.put(&quot;transferToken&quot;, token);</stringProp>
