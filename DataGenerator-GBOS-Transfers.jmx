SELECT top 1 Acc.AccountNumber, pid.TokenizedPAN, Acc.AccountIdentifier, pin.PaymentInstrumentIdentifier 
, cp.FirstName, cp.LastName, cpa.Address1, cpa.Address2, cpa.ZipCode
FROM Account Acc (nolock) left join AccountStatus AccS (nolock) on Acc.AccountStatusKey = AccS.AccountStatusKey
inner join AccountHolder h (nolock) on (h.AccountKey = Acc.AccountKey)
inner join ConsumerProfile cp (nolock) on (cp.ConsumerProfileKey = h.ConsumerProfileKey)
inner join ConsumerProfileIDentity cpi (nolock) on (cpi.ConsumerProfileKey = cp.ConsumerProfileKey)
inner join ConsumerProfileAddress cpa (nolock) on (cp.ConsumerProfileKey = cpa.ConsumerProfileKey)
inner join AccountHolder_PaymentIdentifier ap (nolock) on (ap.AccountHolderKey = h.AccountHolderKey)
inner join PaymentIdentifier pid (nolock) on (pid.PaymentIdentifierKey = ap.PaymentIdentifierKey)
inner join PaymentInstrument pin (nolock) on (pin.PaymentIdentifierKey = pid.PaymentIdentifierKey)
where Acc.AccountIdentifier=&apos;${AccountIdentifier}&apos; 
and AccS.AccountStatusKey = 2</stringProp>
