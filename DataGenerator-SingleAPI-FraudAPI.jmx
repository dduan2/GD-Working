        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="JDBC Request-Get Account" enabled="true">
          <stringProp name="dataSource">GBOS</stringProp>
          <stringProp name="query">select top (${User_Count})  
 a.AccountIdentifier
from Account a (NOLOCK)
INNER JOIN AccountHolder AH (NOLOCK) ON AH.AccountKey = A.AccountKey
inner join Product p (nolock) on (p.ProductKey = a.ProductKey)
inner join Program pg (nolock) on (pg.ProgramKey = p.ProgramKey)
INNER JOIN dbo.ConsumerProfile cp (nolock)   
            ON cp.ConsumerProfileKey = AH.ConsumerProfileKey    
INNER JOIN dbo.AccountHolder_PaymentIdentifier ahpi  (nolock)  
            ON ah.AccountHolderKey = ahpi.AccountHolderKey    
        INNER JOIN dbo.PaymentIdentifier pid  (nolock)  
            ON ahpi.PaymentIdentifierKey = pid.PaymentIdentifierKey    
        INNER JOIN dbo.AccountBalance ab    (nolock)
            ON ab.AccountKey = a.AccountKey  
where 
p.ProductCode = &apos;${PRODUCTCODE}&apos;
and a.CreateDate&gt;&apos;${CreateDate}&apos;
and a.AccountStatusKey=2
and  (a.AccountKey%3)=(CAST(RIGHT(&apos;${machinename}&apos;,1) AS BIGINT)%3) -- ${__machineIP()}
--and ab.AvailableBalance &lt;1
order by NEWID()</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="resultSetHandler">Store as String</stringProp>
          <stringProp name="resultVariable">userList</stringProp>
          <stringProp name="variableNames">AccountIdentifier</stringProp>
          <stringProp name="resultSetMaxRows"></stringProp>
        </JDBCSampler>
        <hashTree>
          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import java.net.InetAddress;
import java.net.UnknownHostException;

def machinename = &quot;&quot;;

try {
	// If the last number of the remote slaves&apos; ip address mod 3 equals to 0, 1 and 2 respectively, then we can use ${__machineIP()} in the SQL directly
	InetAddress lostHost = InetAddress.getLocalHost();
	byte[] addr = lostHost.getAddress();
	
	String ipAddr_lastNum = &quot;&quot;;
	ipAddr_lastNum= addr[3];

    	log.info(&quot;IP Address: &quot; + ipAddr_lastNum);
    
    	if(ipAddr_lastNum == &quot;121&quot;)
    	{
    	 	machinename = &apos;remote03&apos;;
    	}else if(ipAddr_lastNum == &quot;110&quot;){
    		machinename = &apos;remote02&apos;;
    	}else{
    		machinename = &apos;remote01&apos;;
    	}
    	
    	vars.put(&quot;machinename&quot;, machinename);
    
             
    }
    catch (UnknownHostException e) {
        System.out.println(&quot;Host not found: &quot; + e.getMessage());
   }</stringProp>
          </JSR223PreProcessor>
