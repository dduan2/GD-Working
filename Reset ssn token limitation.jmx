        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Get ConsumerProfileIdentityKey from db" enabled="true">
          <stringProp name="dataSource">GBOS</stringProp>
          <stringProp name="queryType">Callable Statement</stringProp>
          <stringProp name="query">DECLARE @AccountList TABLE 
( 
ConsumerProfileIdentityKey BIGINT NOT NULL 
); 

DECLARE @startDate varchar(10)
DECLARE @endDate varchar(10)
set @startDate=CONVERT(VARCHAR(10),GETDATE()-5,120)
SET @endDate=CONVERT(VARCHAR(10),GETDATE(),120)

INSERT INTO @AccountList 
( 
cpi.ConsumerProfileIdentityKey 
) 
SELECT cpi.ConsumerProfileIdentityKey FROM Account [at] (nolock)
INNER JOIN AccountHolder ath (nolock)
on ath.AccountKey= at.AccountKey
INNER JOIN ConsumerProfile cp (nolock)
on ath.ConsumerProfileKey = cp.ConsumerProfileKey
INNER JOIN ConsumerProfileEmail cpe (nolock)
on cpe.ConsumerProfileKey = cp.ConsumerProfileKey
INNER JOIN ConsumerProfileIdentity cpi (nolock)
on cpi.ConsumerProfileKey= cp.ConsumerProfileKey
where 
at.CreateDate between @startDate and @endDate
and 
at.AccountStatusKey=2 and at.ProductKey in (3,4)
and 
cpi.ChangeBy!=&apos;NEXTESTATE\qa_automation_reset_ssn&apos;
and cpe.Email LIKE &apos;%virtual@greendotcorp.com&apos;

INSERT INTO @AccountList 
SELECT ConsumerProfileIdentityKey from ConsumerProfileIdentity (nolock)
WHERE IdentityToken in (SELECT  IdentityToken from ConsumerProfileIdentity (nolock)
 GROUP by IdentityToken
HAVING COUNT(IdentityToken) &gt; 3)
and CreateDate between @startDate and @endDate
and ChangeBy!=&apos;NEXTESTATE\qa_automation_reset_ssn&apos;

SELECT * from @AccountList</stringProp>
