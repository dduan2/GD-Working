          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;

FileWriter fstream = new FileWriter(vars.get (&quot;WORKSPACE_PATH&quot;) + File.separator  + &quot;${RESULT_FILE_NAME_P}&quot;,true);

try{

	for (var row : vars.getObject(&quot;ConsumerProfileIdentityKeyList&quot;))
	{
		String RequestId = java.util.UUID.randomUUID().toString();
		fstream.write(row.get(&quot;ConsumerProfileIdentityKey&quot;) + System.getProperty(&quot;line.separator&quot;));
	}

} catch (FileNotFoundException e) {
    log.error(&quot;FileNotFoundException in Delete csv files&quot;, e);
    //e.printStackTrace();
} catch (IOException e) {
    log.error(&quot;IOException in Delete csv files&quot;, e);
}finally {
    	fstream.flush();
	fstream.close();
}</stringProp>
